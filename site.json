[
{% for item in site.posts %}
	"{{ item.url | slugify }}":	{
		"id": {{ item.id | jsonify}},
		"title": {{ item.title | jsonify}},
		"url": {{ item.url | jsonify}},
		"date": {{ item.date | date_to_rfc822 | jsonify }},
		"categories": {{ item.categories | jsonify }}
	} {% if forloop.last %}{% else %},{% endif %}
{% endfor %}
]
